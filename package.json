{
	"name": "tmlanguage-syntax-highlighter",
	"displayName": "TextMate Syntax Highlighting and Intellisense",
	"description": "Syntax Highlighting and Intellisense for VSCodes's JSON based TextMate grammars",
	"publisher": "RedCMD",
	"version": "2.8.2",
	"engines": {
		"vscode": "^1.87.0"
	},
	"icon": "assets/TextMate-logo-icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/RedCMD/TmLanguage-Syntax-Highlighter.git"
	},
	"bugs": {
		"url": "https://github.com/RedCMD/TmLanguage-Syntax-Highlighter/issues"
	},
	"homepage": "https://marketplace.visualstudio.com/items?itemName=RedCMD.tmlanguage-syntax-highlighter",
	"galleryBanner": {
		"color": "#343A40",
		"theme": "dark"
	},
	"keywords": [
		"textmate",
		"tmlanguage",
		"text mate",
		"syntax highlight",
		"syntax highlighter",
		"tmgrammar-test"
	],
	"categories": [
		"Programming Languages",
		"Snippets",
		"Formatters"
	],
	"capabilities": {
		"virtualWorkspaces": {
			"supported": true,
			"description": "DropEdit and file conversion to CSON/XML do not work in VSCode Web"
		},
		"untrustedWorkspaces": {
			"supported": true,
			"description": "Everything works as far as I know",
			"restrictedConfigurations": []
		}
	},
	"extensionDependencies": [
		"vscode.json-language-features"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"browser": "./out/web/extension.js",
	"scripts": {
		"build": "npm run compile && npm run compile-web && npm run generate-wasm",
		"package": "vsce package --follow-symlinks",
		"publish": "vsce publish --follow-symlinks",
		"compile": "tsc -p ./",
		"compile-web": "webpack --no-devtool --no-watch --mode production",
		"generate-wasm": "cd ./src/tree-sitter/tree-sitter-json/ && tree-sitter generate && tree-sitter build --wasm --output ../../../out/tree-sitter-jsontm.wasm && cd ./../tree-sitter-regex/ && tree-sitter generate && tree-sitter build --wasm --output ../../../out/tree-sitter-regextm.wasm",
		"watch": "tsc -watch -p ./",
		"watch-web": "webpack --watch",
		"test": "npm run test:grammar && npm run test:extension && npm run test:browser",
		"test:update": "npm run build && npm run test:extension:update && npm run test:grammar:update",
		"test:grammar": "textmate-grammar-snap ./syntaxes/**/*.* --outdir ./syntaxes/tests/snaps/ --grammar ./syntaxes/tests/yaml-*.tmLanguage.json",
		"test:grammar:update": "npm run test:grammar -- --updateSnapshot",
		"test:extension": "vscode-test --timeout 30000 --slow 1000 --inline-diffs true",
		"test:extension:update": "npm run test:extension --- --config .vscode-test.update.mjs",
		"test:browser": "vscode-test-web --extensionDevelopmentPath=. --extensionTestsPath=out/web/test.js ./src/test/",
		"open-in-browser": "vscode-test-web --extensionDevelopmentPath=. ."
	},
	"contributes": {
		"languages": [
			{
				"id": "json-textmate",
				"aliases": [
					"JSON TextMate"
				],
				"extensions": [
					".tmLanguage.json",
					".JSON-tmLanguage",
					".tmGrammar.json"
				],
				"filenames": [
					"tmLanguage.json"
				],
				"filenamePatterns": [
					"**/{syntaxes,grammars}/*inject*.json",
					"**/{syntaxes,grammars}/*code{,?}block*.json",
					"**/{syntaxes,grammars}/*.tmLanguage.json",
					"**/{syntaxes,grammars}/*.tmLanguage copy{, [0-9]*}.json",
					"**/{syntaxes,grammars}/*.tmLanguage{, copy} - Copy{, ([0-9]*)}{, - Copy*}.json"
				],
				"firstLine": "{(\"scopeName\"|\"(information_for_contributors|fileTypes)\":\\[)",
				"configuration": "./language-configuration.json",
				"icon": {
					"dark": "assets/TextMate-file-icon.svg",
					"light": "assets/TextMate-file-icon.svg"
				}
			},
			{
				"id": "json-textmate-regex",
				"aliases": [
					"JSON TextMate Regex"
				],
				"extensions": [
					".tmLanguage.json.regex"
				],
				"configuration": "./language-configurations/regex.language-configuration.json",
				"icon": {
					"dark": "assets/TextMate-file-icon.svg",
					"light": "assets/TextMate-file-icon.svg"
				}
			},
			{
				"id": "yaml-textmate",
				"aliases": [
					"YAML TextMate"
				],
				"extensions": [
					".tmLanguage.yaml",
					".tmLanguage.yml",
					".YAML-tmLanguage"
				],
				"configuration": "./language-configurations/yaml.language-configuration.json",
				"icon": {
					"dark": "assets/TextMate-file-icon.svg",
					"light": "assets/TextMate-file-icon.svg"
				}
			},
			{
				"id": "ascii-textmate",
				"aliases": [
					"ASCII PLIST TextMate"
				],
				"extensions": [
					".textmate",
					".tmLanguage",
					".tmLanguage.plist",
					".tm-grammar.plist",
					".property-list",
					".plist"
				],
				"firstLine": "^\\s*{\\s*\\w+\\s*=\\s*.*$",
				"configuration": "./language-configurations/plist.language-configuration.json",
				"icon": {
					"dark": "assets/TextMate-file-icon.svg",
					"light": "assets/TextMate-file-icon.svg"
				}
			},
			{
				"id": "xml",
				"extensions": [
					".tmLanguage",
					".tmGrammar",
					".plist"
				]
			},
			{
				"id": "textmate-scopes",
				"aliases": [
					"TextMate Scopes"
				],
				"extensions": [
					"_scopes.txt"
				],
				"filenamePatterns": [
					"*_scopes.txt"
				],
				"configuration": "./language-configurations/scope.language-configuration.json",
				"icon": {
					"dark": "assets/TextMate-file-icon.svg",
					"light": "assets/TextMate-file-icon.svg"
				}
			},
			{
				"id": "tmgrammar-test",
				"aliases": [
					"TextMate Snap Grammar-Test"
				],
				"extensions": [
					".snap",
					".baseline.txt",
					".hitt"
				],
				"filenamePatterns": [
					"**/tests/baselines/**/*.txt",
					"**/syntaxes/fixtures/baselines/*.txt"
				],
				"firstLine": "^\\S+\\s+SYNTAX\\s+TEST\\s+\"[^\"]+\"(\\s+\"[^\"]+\")?\\s*$|^original file$",
				"configuration": "./language-configurations/snap.language-configuration.json",
				"icon": {
					"dark": "assets/TextMate-file-icon.svg",
					"light": "assets/TextMate-file-icon.svg"
				}
			}
		],
		"grammars": [
			{
				"language": "json-textmate",
				"scopeName": "source.json.textmate",
				"path": "./syntaxes/json.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.json.textmate.regexp": "json-textmate-regex"
				},
				"unbalancedBracketScopes": [
					"invalid.illegal.expected-dictionary-separator.json",
					"invalid.illegal.expected-array-separator.json",
					"invalid.illegal.expected-value-separator.json"
				]
			},
			{
				"language": "json-textmate-regex",
				"scopeName": "source.json.textmate.regexp",
				"path": "./syntaxes/regex.tmLanguage.json"
			},
			{
				"language": "yaml-textmate",
				"scopeName": "source.yaml.textmate",
				"path": "./syntaxes/yaml.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.json.textmate.regexp": "json-textmate-regex",
					"meta.embedded.yaml.textmate.regexp": "json-textmate-regex"
				},
				"unbalancedBracketScopes": [
					"invalid.illegal",
					"storage.type.tag.shorthand.yaml",
					"keyword.control.flow"
				]
			},
			{
				"scopeName": "source.yaml.textmate.regexp",
				"path": "./syntaxes/yaml-regex.tmLanguage.json"
			},
			{
				"language": "ascii-textmate",
				"scopeName": "source.plist.ascii.textmate",
				"path": "./syntaxes/plist-ascii.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.json.textmate.regexp": "json-textmate-regex"
				}
			},
			{
				"scopeName": "source.regexp.oniguruma",
				"path": "./syntaxes/plist-ascii-regex.tmLanguage.json"
			},
			{
				"scopeName": "source.plist.ascii.textmate.scopes",
				"path": "./syntaxes/plist-ascii-scopes.tmLanguage.json"
			},
			{
				"language": "textmate-scopes",
				"scopeName": "source.textmate.scopes",
				"path": "./syntaxes/scopes.tmLanguage.json",
				"balancedBracketScopes": [
					"replace.capture"
				]
			},
			{
				"language": "tmgrammar-test",
				"scopeName": "source.textmate.tmgrammar",
				"path": "./syntaxes/tmgrammar-test.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.typescript": "typescript",
					"meta.embedded.block.json.textmate": "json-textmate",
					"meta.embedded.block.json.textmate.regexp": "json-textmate-regex"
				}
			},
			{
				"scopeName": "source.textmate.tmgrammar.injection",
				"path": "./syntaxes/tmgrammar-injection.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.tmgrammar": "tmgrammar-test"
				},
				"injectTo": [
					"source",
					"text",
					"markdown"
				]
			},
			{
				"scopeName": "source.textmate.tmgrammar.blocks.injection",
				"path": "./syntaxes/tmgrammar-blocks-injection.tmLanguage.json",
				"embeddedLanguages": {
					"meta.embedded.tmgrammar": "tmgrammar-test"
				},
				"injectTo": [
					"source",
					"text",
					"markdown"
				]
			},
			{
				"scopeName": "markdown.json.textmate.codeblock",
				"path": "./syntaxes/markdown.tmLanguage.json",
				"injectTo": [
					"text.html.markdown"
				],
				"embeddedLanguages": {
					"meta.embedded.block.json.textmate": "json-textmate",
					"meta.embedded.block.json.textmate.regexp": "json-textmate-regex"
				}
			},
			{
				"scopeName": "text.searchResult.json.textmate",
				"path": "./syntaxes/searchResult.tmLanguage.json",
				"injectTo": [
					"text.searchResult"
				],
				"embeddedLanguages": {
					"meta.embedded.json.textmate": "json-textmate",
					"meta.embedded.json.textmate.regexp": "json-textmate-regex"
				}
			}
		],
		"snippets": [
			{
				"language": "json-textmate-regex",
				"path": "./snippets/textmate-regex.code-snippets"
			},
			{
				"language": "textmate-scopes",
				"path": "./snippets/textmate-scopes.code-snippets"
			}
		],
		"configurationDefaults": {
			"[json-textmate]": {
				"editor.maxTokenizationLineLength": 600000,
				"editor.suggest.insertMode": "replace",
				"editor.suggest.preview": true,
				"editor.quickSuggestions": {
					"strings": "on",
					"other": "on"
				},
				"editor.inlineSuggest.syntaxHighlightingEnabled": true,
				"editor.defaultFormatter": "RedCMD.tmlanguage-syntax-highlighter",
				"editor.comments.insertSpace": false,
				"editor.wrapOnEscapedLineFeeds": true
			},
			"[json-textmate-regex]": {
				"editor.snippetSuggestions": "top",
				"editor.suggest.insertMode": "replace",
				"editor.suggest.snippetsPreventQuickSuggestions": true,
				"editor.suggest.preview": true,
				"editor.suggest.matchOnWordStartOnly": false,
				"editor.quickSuggestions": {
					"strings": "inline",
					"other": "inline",
					"comments": "off"
				},
				"editor.inlineSuggest.syntaxHighlightingEnabled": true,
				"editor.inlineSuggest.enabled": true,
				"editor.wrapOnEscapedLineFeeds": true
			},
			"[yaml-textmate]": {
				"editor.tabSize": 2,
				"editor.insertSpaces": true,
				"editor.quickSuggestions": {
					"strings": "inline",
					"other": "inline",
					"comments": "off"
				}
			},
			"[textmate-scopes]": {
				"editor.snippetSuggestions": "top",
				"editor.suggest.insertMode": "replace",
				"editor.quickSuggestions": {
					"strings": "inline",
					"other": "inline",
					"comments": "inline"
				}
			},
			"[tmgrammar-test]": {
				"editor.tabSize": 1
			}
		},
		"configuration": [
			{
				"title": "JSON TextMate",
				"properties": {
					"json.textmate.formattingStyle": {
						"type": "string",
						"enum": [
							"tight",
							"default"
						],
						"enumDescriptions": [
							"Leave no space between brackets",
							"Separate brackets with a space"
						],
						"default": "default",
						"scope": "resource"
					},
					"json.textmate.sortOrder": {
						"markdownDescription": "Order in which items are sorted when running refactor `Sort JSON Keys` ([settings.json](command:workbench.action.openSettingsJson))",
						"type": "array",
						"items": {
							"type": "string",
							"uniqueItems": true
						},
						"default": [
							"version",
							"$schema",
							"match",
							"begin",
							"end",
							"captures",
							"beginCaptures",
							"endCaptures",
							"whileCaptures",
							"name",
							"contentName",
							"scopeName",
							"fileTypes",
							"firstLineMatch",
							"foldingStartMarker",
							"foldingStopMarker",
							"injectionSelector",
							"injections",
							"include",
							"applyEndPatternLast",
							"patterns",
							"repository",
							"uuid"
						],
						"scope": "resource"
					}
				}
			}
		],
		"jsonLanguageParticipants": [
			{
				"languageId": "json-textmate"
			}
		],
		"yamlLanguageParticipants": [
			{
				"languageId": "yaml-textmate"
			}
		],
		"jsonValidation": [
			{
				"fileMatch": [
					"*.tmLanguage.json",
					"*.tmGrammar.json",
					"*.JSON-tmLanguage"
				],
				"url": "./vscode.tmLanguage.schema.json"
			}
		],
		"yamlValidation": [
			{
				"fileMatch": [
					"*.tmLanguage.yaml",
					"*.tmLanguage.yml",
					"*.YAML-tmLanguage",
					"*.yaml-tmLanguage"
				],
				"url": "./vscode.tmLanguage.schema.json"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "TextMate",
					"title": "TextMate",
					"icon": "assets/TextMate-file-icon.svg"
				}
			]
		},
		"views": {
			"TextMate": [
				{
					"id": "TextMate-Call",
					"name": "Call Details",
					"contextualTitle": "TextMate Call Details",
					"icon": "assets/TextMate-file-icon.svg",
					"visibility": "visible",
					"initialSize": 10
				},
				{
					"id": "TextMate",
					"name": "Call Stack",
					"contextualTitle": "TextMate Call Stack",
					"icon": "assets/TextMate-file-icon.svg",
					"visibility": "visible"
				}
			]
		},
		"menus": {
			"editor/context": [
				{
					"command": "textmate.callstack",
					"group": "0_navigation@9"
				}
			],
			"view/title": [
				{
					"command": "textmate.refresh",
					"group": "navigation@0",
					"when": "view == TextMate"
				},
				{
					"command": "textmate.tree-view",
					"group": "navigation@2",
					"when": "view == TextMate && textmate.call.view == 'list'"
				},
				{
					"command": "textmate.list-view",
					"group": "navigation@2",
					"when": "view == TextMate && textmate.call.view != 'list'"
				},
				{
					"command": "textmate.find",
					"group": "navigation@4",
					"when": "view == TextMate"
				},
				{
					"command": "editor.action.inspectTMScopes",
					"group": "navigation@6",
					"when": "view == TextMate || view == TextMate-Call"
				},
				{
					"command": "textmate.tree-view",
					"group": "viewAs",
					"when": "view == TextMate"
				},
				{
					"command": "textmate.list-view",
					"group": "viewAs",
					"when": "view == TextMate"
				},
				{
					"command": "textmate.follow.cursor",
					"group": "config@1",
					"when": "view == TextMate"
				}
			],
			"view/item/context": [
				{
					"command": "textmate.refresh",
					"group": "inline@1",
					"when": "view == TextMate && viewItem == document"
				},
				{
					"command": "textmate.copytoclipboard.grammar",
					"group": "inline@3",
					"when": "view == TextMate && viewItem == root"
				},
				{
					"command": "textmate.goto.file",
					"group": "inline@5",
					"when": "view == TextMate || view == TextMate-Call"
				},
				{
					"command": "textmate.goto.grammar",
					"group": "inline@7",
					"when": "view == TextMate || view == TextMate-Call"
				},
				{
					"command": "textmate.copytoclipboard.grammar",
					"group": "0_navigation@3",
					"when": "view == TextMate && viewItem == root"
				},
				{
					"command": "textmate.goto.file",
					"group": "0_navigation@5",
					"when": "view == TextMate || view == TextMate-Call"
				},
				{
					"command": "textmate.goto.grammar",
					"group": "0_navigation@7",
					"when": "view == TextMate || view == TextMate-Call"
				}
			]
		},
		"commands": [
			{
				"command": "textmate.callstack",
				"title": "Show TextMate Calling Stack",
				"shortTitle": "TextMate Call Stack",
				"category": "TextMate",
				"icon": "assets/TextMate-file-icon.svg"
			},
			{
				"command": "textmate.refresh",
				"title": "Refresh",
				"category": "TextMate",
				"icon": "$(refresh)"
			},
			{
				"command": "textmate.follow.cursor",
				"title": "Follow Cursor",
				"category": "TextMate",
				"icon": "assets/TextMate-file-icon.svg"
			},
			{
				"command": "textmate.copytoclipboard.grammar",
				"title": "Copy <grammar state> to clipboard (JSON)",
				"category": "TextMate",
				"icon": "$(copy)"
			},
			{
				"command": "textmate.call.details",
				"title": "Show Call Details",
				"category": "TextMate",
				"icon": "$(link-external)"
			},
			{
				"command": "textmate.goto.file",
				"title": "Go To File",
				"category": "TextMate",
				"icon": "$(go-to-file)"
			},
			{
				"command": "textmate.goto.grammar",
				"title": "Go To Grammar",
				"category": "TextMate",
				"icon": "$(references)"
			},
			{
				"command": "textmate.list-view",
				"title": "View as List",
				"category": "TextMate",
				"icon": "$(list-tree)"
			},
			{
				"command": "textmate.tree-view",
				"title": "View as Tree",
				"category": "TextMate",
				"icon": "$(list-flat)"
			},
			{
				"command": "textmate.find",
				"title": "Find",
				"category": "TextMate",
				"icon": "$(search)"
			},
			{
				"command": "editor.action.inspectTMScopes",
				"title": "Developer: Inspect Editor Tokens and Scopes",
				"category": "TextMate",
				"icon": "$(inspect)"
			},
			{
				"command": "textmate.convertFileToJSON",
				"title": "Convert file to JSON",
				"category": "TextMate",
				"icon": "assets/TextMate-file-icon.svg"
			},
			{
				"command": "textmate.convertFileToYAML",
				"title": "Convert file to YAML",
				"category": "TextMate",
				"icon": "assets/TextMate-file-icon.svg"
			},
			{
				"command": "textmate.convertFileToXML",
				"title": "Convert file to XML",
				"category": "TextMate",
				"icon": "assets/TextMate-file-icon.svg",
				"enablement": "!isWeb"
			},
			{
				"command": "textmate.convertFileToASCII",
				"title": "Convert file to ASCII (plist)",
				"category": "TextMate",
				"icon": "assets/TextMate-file-icon.svg"
			},
			{
				"command": "textmate.convertFileToCSON",
				"title": "Convert file to CSON",
				"category": "TextMate",
				"icon": "assets/TextMate-file-icon.svg",
				"enablement": "!isWeb"
			}
		]
	},
	"dependencies": {
		"@syntropiq/libpcre-ts": "file:node_modules_linked/@syntropiq/libpcre-ts",
		"cson-parser": "^4.0.9",
		"cson2json": "file:node_modules_linked/cson2json",
		"date-and-time": "3.6.0",
		"oniguruma-parser-cjs": "^0.0.1",
		"oniguruma-to-es": "^4.3.4",
		"plist": "^3.1.0",
		"vscode-onigmo": "^2.0.1",
		"vscode-oniguruma": "1.7.0",
		"web-tree-sitter": "file:node_modules_linked/web-tree-sitter",
		"yaml": "^2.8.2"
	},
	"devDependencies": {
		"@types/assert": "^1.5.11",
		"@types/emscripten": "^1.41.5",
		"@types/mocha": "^10.0.10",
		"@types/node": "^25.0.5",
		"@types/plist": "^3.0.5",
		"@types/vscode": "^1.87.1",
		"@types/webpack-env": "^1.18.8",
